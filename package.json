{
  "name": "meriyah-printer",
  "version": "1.0.0",
  "main": "dist/bundle.js",
  "module": "dist/bundle.mjs",
  "types": "src/index.ts",
  "license": "MIT",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "typecheck": "tsc",
    "test": "jest",
    "bundle:esm": "esbuild ./src/index.ts --outfile=dist/bundle.mjs --bundle --format=esm",
    "bundle:cjs": "esbuild ./src/index.ts --outfile=dist/bundle.js --bundle --format=cjs",
    "bundle": "yarn bundle:esm && yarn bundle:cjs"
  },
  "devDependencies": {
    "esbuild": "^0.13.3",
    "jest": "^27.2.4",
    "meriyah": "^4.2.0",
    "typescript": "^4.4.3"
  }
}
